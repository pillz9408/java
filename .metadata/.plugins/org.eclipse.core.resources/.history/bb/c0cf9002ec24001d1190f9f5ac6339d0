package thebrains.pim.model;

public class Member { //Model 객체에서 Getter/Setter 메소드가 가장 많이 사용.
	private int mno; //member number -유일한 식별자= unique : primary
	private String email; // email을 id로 사용 - unique
	private String pw; // 길이15 
	private String name; // 길이 15
	private String phone; // 길이 20
	private String address; // 길이 50
	

}
