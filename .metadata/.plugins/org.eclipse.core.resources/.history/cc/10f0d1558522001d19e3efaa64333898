package kr.brains063;

public class Car {
	String model;
	private int gas;
	
	//Setter/Getter Method;
	void setGas(int gas) {
		this.gas = gas;	
	}
	int getGas() {
		return gas;
	}
	boolean isLeftGas() { 
		if(gas == 0) {
			System.out.print("gas가 없습니다 \n");
			return false;
		}
		else {
			System.out.print("gas가 있습니다 \n");
			return true;
		}
		
	}

}
